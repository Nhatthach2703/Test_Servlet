<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CalServlet</servlet-name>
        <servlet-class>Controller.CalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeoServlet</servlet-name>
        <servlet-class>Controller.GeoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmojiServlet</servlet-name>
        <servlet-class>Controller.EmojiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CalServlet</servlet-name>
        <url-pattern>/CalServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeoServlet</servlet-name>
        <url-pattern>/GeoServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmojiServlet</servlet-name>
        <url-pattern>/EmojiServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
